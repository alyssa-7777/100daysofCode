  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "BCD\n",
      "AAA\n"
     ]
    }
   ],
   "source": [
    "S = input()\n",
    "result = []\n",
    "for i, ch in enumerate(S, 1):\n",
    "    chr_index = ord(ch) - i % 26\n",
    "    if chr_index < ord('A'):\n",
    "        chr_index += 26\n",
    "    result.append(chr(chr_index))\n",
    "\n",
    "print(\"\".join(result))"
   ]
  },
